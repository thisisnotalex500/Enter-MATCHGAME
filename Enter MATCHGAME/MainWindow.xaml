            <ColumnDefinition Width="100*"/>
            <ColumnDefinition Width="100*"/>
            <ColumnDefinition Width="100*"/>
        </Grid.ColumnDefinitions>
        <TextBlock x:Name="TimeTextBlock_MouseDown1" Grid.Column="1" HorizontalAlignment="Center" Grid.Row="1" Text="TextBlock" VerticalAlignment="Top" Width="60" FontSize="30" FontFamily="Times New Roman" Margin="0,42,0,0" MouseDown="TextBlock_MouseDown"/>
        <TextBlock x:Name="TimeTextBlock_MouseDown" Grid.ColumnSpan="2" HorizontalAlignment="Left" Margin="25,0,0,0" Grid.Row="2" Text="TextBox" VerticalAlignment="Center" Width="120" FontSize="30" FontFamily="Times New Roman" MouseDown="TextBlock_MouseDown"/>
