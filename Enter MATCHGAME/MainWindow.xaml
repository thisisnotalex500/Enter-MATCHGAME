        <TextBlock x:Name="TimeTextBlock_MouseDown5" HorizontalAlignment="Left" Grid.Row="3" Text="TextBlock" VerticalAlignment="Top" Width="60" FontSize="30" FontFamily="Times New Roman" Margin="25,29,0,0" Grid.ColumnSpan="2" MouseDown="TextBlock_MouseDown"/>
        <TextBlock x:Name="TimeTextBlock_MouseDown6" HorizontalAlignment="Center" Grid.Row="1" Text="TextBlock" VerticalAlignment="Top" Width="60" FontSize="30" FontFamily="Times New Roman" Margin="0,44,0,0" Grid.Column="2" MouseDown="TextBlock_MouseDown"/>
